<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude specific shared preferences that contain sensitive data -->
        <exclude domain="sharedpref" path="firebase_config.xml" />
        <exclude domain="sharedpref" path="battery_repair_prefs.xml" />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude specific shared preferences that contain sensitive data -->
        <exclude domain="sharedpref" path="firebase_config.xml" />
        <exclude domain="sharedpref" path="battery_repair_prefs.xml" />
    </device-transfer>
</data-extraction-rules>